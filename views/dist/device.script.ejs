<% if (scripts.length) { %>
    <nav class="navigation navigation_scripts">
        <ul class="navigation-list navigation-list_scripts">
            <% scripts.forEach(function (e) { %>
                <li class="divice-info navigation-list__item device-info device-info_script">
                    <div class="device-info-intro">
                        <% if (e.type) { %>
                            <img class="device-info-intro__type" src="./images/i_<%= e.type %>_<%= e.isActive ? 'a' : 'da' %>ctive.svg">
                        <% } %>
                        <div "device-info-intro-data">
                            <% if (e) { %>
                                <div class="device-info-intro__name">
                                    <%= e.scriptName %>
                                </div>
                            <% } %>
                            <% if (e) { %>
                                <div class="device-info-intro__state">
                                    <%= e.state %>
                                </div>
                            <% } %>
                        </div>
                    </div>
                </li>
            <% }); %>
        </ul>
    </nav>
<% } %>