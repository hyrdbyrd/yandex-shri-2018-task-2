.db-arrows
    box-sizing: border-box
    background: $color-lightGray
    z-index: 1
    width: inherit
    height: 40px
    border-radius: 20px 20px 0 0
    display: flex
    justify-content: center
    align-items: center
    margin-bottom: -25px
    transition: $animate

@media (max-width: 1099px)
    .db-arrows
        display: none